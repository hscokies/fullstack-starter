{
    "$schema": "https://json.schemastore.org/prettierrc",
    "plugins": ["@prettier/plugin-oxc"],
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 4,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true,
    "printWidth": 120,
    "semi": true,
    "trailingComma": "es5",
    "semiColons": true,
    "endOfLine": "lf",
    "arrowParens": "avoid"
}
